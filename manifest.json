{
    "name": "Weather App",
    "description": "Displays weather according to user's location",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
      "default_popup": "./index.html",
      "default_icon": "icons/icon.png"
    },
    "icons": {
      "16": "icons/icon.png",
      "64": "icons/icon.png",
      "128": "icons/icon.png"
    },
    "permissions": ["activeTab", "geolocation"]
  }